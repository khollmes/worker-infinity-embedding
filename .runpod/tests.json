{
  "tests": [
    {
      "name": "Reranking Request",
      "input": {
        "model": "BAAI/bge-reranker-v2-m3",
        "query": "warranty coverage",
        "docs": [
          "Product A has 2-year warranty",
          "Product B is available in red and blue",
          "All electronics include standard 1-year warranty"
        ],
        "return_docs": true
      },
      "timeout": 1200000
    }
  ],
  "config": {
    "gpuTypeId": "NVIDIA RTX A5000",
    "gpuCount": 1,
    "allowedCudaVersions": ["12.7", "12.6", "12.5", "12.4"],
    "env": [
      {
        "key": "MODEL_NAMES",
        "value": "BAAI/bge-reranker-v2-m3"
      }
    ]
  }
}
